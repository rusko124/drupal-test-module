{#
/**
 * @file discount_entity.html.twig
 * Default theme implementation to present Discount entity data.
 *
 * This template is used when viewing Discount entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_discount_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('discount_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
